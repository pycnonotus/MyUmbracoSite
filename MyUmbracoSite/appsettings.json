{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Server=db;Database=master;User=sa;Password=YourStrong!Pass123;TrustServerCertificate=True;"
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "c227f7fd-4a90-4cff-afc6-45bd10534185",
        "SanitizeTinyMce": true,
        "UseHttps": true
      },
      "WebRouting": {
        "UmbracoApplicationUrl": "https://pycnonotus.dev"
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    }
  },
  "OpenIddict": {
    "Server": {
      "Issuer": "https://pycnonotus.dev",
      "AllowInsecureHttp": false,
      "DisableAccessTokenEncryption": true,
      "UseReferenceAccessTokens": false
    }
  }
}
